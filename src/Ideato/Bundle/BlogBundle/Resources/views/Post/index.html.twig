{% extends 'IdeatoBlogBundle::layout.html.twig' %}
{% block content %}
    <div>
    {% for post in posts %}
            <h1>{{ post.title }}</h1>
            <h2>publication date: {{ post.publicationDate | date}}</h2>
            <h4>{{ post.content }}</h4>
    {% endfor %}
    </div>
{% endblock %}